<h1>Articles posting</h1>

<%= link_to "Write a new article", new_article_path %>

<% @articles.each do |article| %>
 <p><%= article.title %></p>
 <p><%= article.content %></p>
 <%= link_to "Go to see", article_path(article) %>
 <%= link_to "Delete", article_path(article), method: :delete %>
<% end %>
