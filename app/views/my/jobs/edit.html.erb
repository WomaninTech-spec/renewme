<%= simple_form_for([current_user, @job], url: my_user_job_path, remote: true) do |f| %>

  <%= f.input :title, label: "Enter the title of the job" %>

  <%= f.input :description, label: "Enter the content of the job" %>
  <%= f.input :visible %>
  <%= f.button :submit, class: 'btn btn-outline-primary' %>
<% end -%>

